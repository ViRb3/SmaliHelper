<Window x:Class="Smali_Helper_v2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Smali_Helper_v2"
        mc:Ignorable="d"
        Title="Smali Helper v2" Height="447.4" Width="792.6">
    <Grid>
        <GroupBox x:Name="groupBox" Header="" HorizontalAlignment="Stretch" Margin="193,0,10,10" VerticalAlignment="Stretch" Height="Auto" Width="Auto">
            <DocumentViewer x:Name="documentViewer" HorizontalAlignment="Stretch" Margin="0,0,0,0" VerticalAlignment="Stretch">
                <DocumentViewer.Resources>
                    <Style TargetType="ContentControl">
                        <Setter Property="Visibility" Value="Collapsed" />
                    </Style>

                    <Style TargetType="ToolBar">
                        <Setter Property="Visibility" Value="Collapsed" />
                    </Style>
                </DocumentViewer.Resources>
            </DocumentViewer>
        </GroupBox>
        <TreeView x:Name="treeView" Height="Auto" Margin="10,10,0,10" VerticalAlignment="Stretch" SelectedItemChanged="TreeView_OnSelectedItemChanged" HorizontalAlignment="Left" Width="178"/>

    </Grid>
</Window>
